<p id="notice"><%= notice %></p>

<h1>Notes</h1>

<table>
  <thead>
    <tr>
      <th>Label</th>
      <th>Content</th>
      <th>Time stamp</th>
      <th>Index</th>
      <th>Level</th>
      <th colspan="3"></th>
      <th>parent_id</th>
    </tr>
  </thead>

  <tbody>
    <% @notes.each do |note| %>
      <tr>
        <td><%= note.label %></td>
        <td><%= note.content %></td>
        <td><%= note.time_stamp %></td>
        <td><%= note.index %></td>
        <td><%= note.level %></td>
        <td><%= note.parent_id %></td>
        <td><%= link_to 'Show', note %></td>
        <td><%= link_to 'Edit', edit_note_path(note) %></td>
        <td><%= link_to 'Destroy', note, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Note', new_note_path %>
